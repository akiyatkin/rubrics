{
	"name":"rubrics",
	"child":{
		"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div><div id='rubricsgallery'></div>"],
		"divs":{
			"rubricscontent":{
				"tpl":"-rubrics/rubrics.tpl",
				"tplroot":"404",
				"parsedtpl":"{crumb.name}",
				"autoedittpl":{"fast": "~{Config.get(:rubrics).main}/{crumb.name}"},
				"showanimate":true,
				"layers":{
					"seojsontpl":"~{Config.get(:rubrics).main}/{crumb.name}.json",
					"istpl":"{infrajs.names.rubrics.childs[crumb.name]??:yes}",
					"tplcheck":true,
					"autoedittpl":{"fast":"~{Config.get(:rubrics).main}/{crumb.name}"},
					"tpltpl":"-rubrics/?type={Config.get(:rubrics).main}&id={crumb.name}&show"
				}
			},
			"rubricsgallery":{
				"tpl":"-rubrics/gallery.tpl",
				"jsontpl":"-rubrics/?type={Config.get(:rubrics).main}&gallery&id={crumb.name}"
			}
		}
	},
	"childs":{
		"info":{
			"config":{ },
			"seojsontpl":"~{crumb.name}/{crumb.name}.json",
			"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div><div id='rubricsgallery'></div>"],
			"tplroot":"root",
			"autoedittpl":{"fast":"~{crumb.name}/"},
			"divs":{
				"rubricstitle":{
					"tpl":["<h1>{Config.get(:rubrics).list[crumb.name].title}</h1>"],
					"tplroot":"root"
				},
				"rubricscontent":{
					"jsontpl":"-rubrics/?type={crumb.name}&list&lim=0,50",
					"tpl":"-rubrics/rubrics.tpl",
					"tplroot":"PAGES",
					"child":{
						"showanimate":true,
						"tpltpl":"-rubrics/?type={crumb.parent.name}&show&id={crumb.name}",
						"autoedittpl":{ "fast":"~{crumb.parent.name}/{crumb.name}" },
						"divs":{
							"rubricstitle":{
								"tpl":[""]
							},
							"rubricsgallery":{
								"tpl":"-rubrics/gallery.tpl",
								"jsontpl":"-rubrics/?type={crumb.parent.name}&gallery&id={crumb.name}"
							}
						}
					}
				}
			}
		},
		"files":{
			"seojsontpl":"~seo/{crumb.name}.json",
			"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div>"],
			"tplroot":"root",
			"autoedittpl":{"fast":"~{crumb.name}/"},
			"divs":{
				"rubricstitle":{
					"tpl":["<h1>{Config.get(:rubrics).list[crumb.name].title}</h1>"],
					"tplroot":"root"
				},
				"rubricscontent":{
					"jsontpl":"-rubrics/?type={crumb.name}&list&lim=0,50",
					"tpl":"-rubrics/rubrics.tpl",
					"tplroot":"FILES"
				}
			}
		},
		"list":{
			"config":{ },
			"tpl":["<div id='rubricstitle'></div><div id='rubricscontent'></div><div id='rubricsgallery'></div>"],

			
			"divs":{
				"rubricstitle":{
					"tpl":["<h1>{Config.get(:rubrics).list[crumb.name].title}</h1>"],
					"tplroot":"root"
				},
				"rubricscontent":{
					"tpl":"-rubrics/rubrics.tpl",
					"tplroot":"PAGES",
					"autoedittpl":{"fast":"~{crumb.name}/"},
					"jsontpl":"-rubrics/?type={crumb.name}&list&lim=0,50",
					"child":{
						"showanimate":true,
						"tpltpl":"-rubrics/?type={crumb.parent.name}&show&id={crumb.name}",
						"autoedittpl":{ "fast":"~{crumb.parent.name}/{crumb.name}" },
						"divs":{
							"rubricstitle":{
								"tpl":["<a href='/{crumb.parent.name}'>{Config.get(:rubrics).list[crumb.parent.name].title}</a>"],
								"tplroot":"root"
							},
							"rubricsgallery":{
								"tpl":"-rubrics/gallery.tpl",
								"jsontpl":"-rubrics/?type={crumb.parent.name}&gallery&id={crumb.name}"
							}
						}
					}
				}
			}
		}
	}
}
